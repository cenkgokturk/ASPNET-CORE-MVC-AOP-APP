@model List<ASPNETAOP.Models.AdminPage>

@{
    ViewData["Title"] = "UserList";
}

<h1>Users</h1>

<table>
    <tr>
        <th>User Name</th>
        <th>Mail</th>
        <th></th>
    </tr>
    @foreach(var user in Model)
    {
    <tr>
        <td>@user.Username</td>
        <td>@user.Usermail</td>
    </tr>
    }
</table>